{
  "name": "scratch-desktop",
  "productName": "Scratch Desktop (TechLAB4Kids release)",
  "description": "Scratch 3.24.0.TL4K03 as a self-contained desktop application",
  "author": "Scratch Foundation",
  "version": "3.24.6",
  "license": "BSD-3-Clause",
  "scripts": {
    "clean": "rimraf ./dist",
    "clean2": "rimraf ./dist ./static/assets",
    "start": "mkdirp ./dist && electron-webpack dev --bail --display-error-details --env.minify=false --no-progress",
    "compile": "export NODE_OPTIONS=--max-old-space-size=6500 && mkdirp ./dist && electron-webpack --bail --display-error-details --env.minify=false --no-progress",
    "fetch": "rimraf ./static/assets/ && mkdirp ./static/assets/ && node ./scripts/fetchMediaLibraryAssets.js",
    "build": "npm run build:dev",
    "build:dev": "npm run compile && npm run doBuild -- --mode=dev",
    "build:dir": "npm run compile && npm run doBuild -- --mode=dir",
    "build:dist": "export NODE_OPTIONS=--max-old-space-size=6500 && npm run compile && npm run doBuild -- --mode=dist",
    "doBuild": "node ./scripts/electron-builder-wrapper.js --linux",
    "dist": "npm run clean && npm run preBuild && npm run compile && npm run fetch && npm run doBuild -- --mode=dist",
    "distDist": "npm run clean && npm run preBuild && npm run compile  && npm run doBuild -- --mode=dist",
    "test": "npm run test:lint",
    "test:lint": "eslint --cache --color --ext .jsx,.js .",
      "preBuild": "npm i && npm link scratch-gui scratch-vm"
  },
  "build": {
    "asar": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LLK/scratch-desktop.git"
  },
